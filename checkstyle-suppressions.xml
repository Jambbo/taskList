<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">
<suppressions>
    <suppress checks="MissingJavadocMethod"/>
    <suppress checks="JavadocPackage"/>
    <suppress checks="JavadocVariable"/>
    <suppress checks="DesignForExtension"/>
    <suppress checks="HideUtilityClassConstructor"/>
    <suppress checks="MagicNumber"/> <!--Использование случайных чисел которые не переменные строгое правило-->
    <suppress checks="HiddenField"/> <!--Тут мы не хотим проверку на хидденфилд(проверка на то что в конструкторе совпадает названия полей с названиями полей классов -->
</suppressions>


